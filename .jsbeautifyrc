{
  "html": {
    "brace_style": "collapse",
    "indent_char": " ",
    "indent_scripts": "normal",
    "indent_size": 2,
    "indent_level": 0,
    "indent_with_tabs": true,
    "max_preserve_newlines": 1,
    "preserve_newlines": true,
    "unformatted": ["a", "sub", "sup", "b", "i", "u"],
    "wrap_line_length": 0
  },
  "css": {
    "indent_char": " ",
    "indent_size": 2,
    "indent_level": 0,
    "indent_with_tabs": true,
    "max_preserve_newlines": 1,
    "preserve_newlines": true,
  },
  "js": {
    "indent_char": " ",
    "indent_size": 2,
    "indent_level": 0,
    "indent_with_tabs": true,
    "max_preserve_newlines": 2,
    "preserve_newlines": true,
    "jslint_happy": true
  },
}
